.ASM.OBJ:
    MASM /DOEM$(OEM) /DVER$(VER) /I..\INC $*.ASM,$@,$*.LST;

DEBUG.OBJ: DEBUG.ASM

DEBASM.OBJ: DEBASM.ASM

DEBUASM.OBJ: DEBUASM.ASM

DEBCOM1.OBJ: DEBCOM1.ASM

DEBCOM2.OBJ: DEBCOM2.ASM

DEBCONST.OBJ: DEBCONST.ASM

DEBDATA.OBJ: DEBDATA.ASM

DEBMES.OBJ: DEBMES.ASM

DEBUG.EXE: DEBUG.OBJ DEBCOM1.OBJ DEBCOM2.OBJ DEBUASM.OBJ DEBASM.OBJ \
    DEBCONST.OBJ DEBDATA.OBJ DEBMES.OBJ
